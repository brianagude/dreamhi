<section class="background-text">
    <%= image_tag("logoglobe.png") %>
</section>

<section class="projects mt">
    <div class="container">
        <div class="card video-card">
            <iframe width="100%" height="500px" src="https://www.youtube.com/embed/c5rYRNV-sl0" controls="0" frameborder="0" autoplay="1" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
        <div class="lookbook_imgs grid">
            <% @projects.each do |project| %>
                <div> <%= image_tag project.image_1.medium.url %> </div>

                <% if project.image_2.present? %>
                    <div> <%= image_tag project.image_2.medium.url %> </div>
                <% end %>

                <% if project.image_3.present? %>
                    <div> <%= image_tag project.image_3.medium.url %> </div>
                <% end %>

                <% if project.image_4.present? %>
                    <div> <%= image_tag project.image_4.medium.url %> </div>
                <% end %>

                <% if project.image_5.present? %>
                    <div> <%= image_tag project.image_5.medium.url %> </div>
                <% end %>

                <% if project.image_6.present? %>
                    <div> <%= image_tag project.image_6.medium.url %>  </div>
                <% end %>

                <% if project.image_7.present? %>
                    <div> <%= image_tag project.image_7.medium.url %> </div>
                <% end %>

                <% if project.image_8.present? %>
                    <div> <%= image_tag project.image_8.medium.url %> </div>
                <% end %>

                <% if project.image_9.present? %>
                    <div> <%= image_tag project.image_9.medium.url %> </div>
                <% end %>

                <% if project.image_10.present? %>
                    <div> <%= image_tag project.image_10.medium.url %> </div>
                <% end %>
            <% end %>
        </div>
    </div>
</section>
